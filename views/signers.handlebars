<p id="signers">Our Supporters 🎗</p>
<ul class="petionerslist">
    {{#results}}
        <li>
            {{#if url}}
            <a href=`{{url}}`>{{first}}{{last}}</a> {{age}} 
            <a href="/signers/{{city}}">{{city}} </a>
                        {{else}}
{{first}} {{last}} {{age}} <a href="/signers/:{{city}}">{{city}} </a>
            {{/if}}
            </li>
    {{/results}}
</ul>